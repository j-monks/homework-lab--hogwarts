  <div class="list">
  <ul>
    <% for student in @students %>
      <li>
        <%= student.first_name %>
        <%= student.last_name %>
        <% @house = Student.find_house(student.house_id) %>
        <%= @house.name %>
        <%= student.age %>
      </li>
      <hr>
    <% end %>
  </ul>
</div>
